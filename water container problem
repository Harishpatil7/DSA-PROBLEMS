class Solution:
    def maxArea(self, height: List[int]) -> int:
        maxwt=0
        i=0
        j=len(height)-1
        while(i<j):
            width=j-i
            height1=min(height[i],height[j])
            area=width*height1
            maxwt=max(maxwt,area)
            if(height[i]<height[j]):
                i+=1
            else:
                j-=1
        return maxwt
