class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int max=-99999;
        int current_sum=0;
        for(int i:nums)
        {
            current_sum+=i;
            max=current_sum>max?current_sum:max;
            if(current_sum<0)
            {
                current_sum=0;
            }

        }
        return max;
        
    }
};
